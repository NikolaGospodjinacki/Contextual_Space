# Development environment configuration
aws_region  = "us-east-1"
environment = "dev"
app_name    = "contextual-space"

# DynamoDB - pay per request for dev (cost effective)
dynamodb_billing_mode = "PAY_PER_REQUEST"

# App Runner - minimal resources for dev
app_runner_cpu    = "256"
app_runner_memory = "512"
backend_port      = 3001

# Domain (leave empty for now)
domain_name = ""
